<div className="form-group">
    {{#unless vec}}<span className="form-label">
      <strong>{{pascal_case field_name}}</strong>
    </span>{{/unless}}
    <div className="form-control">
        <SlInput name={`{{field_name~}}{{#if vec}}[${i}]{{/if}}`} value={ {{~entry_name}}.{{field_name~}}{{#if vec}}[i]{{/if}} } onSlChange={ (e: any) => handleChange({event: e}) }{{#if vec}} key={i}{{/if}} {{#if required}} required{{/if}} />
    </div>
</div>